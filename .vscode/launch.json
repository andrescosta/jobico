    "version": "0.2.0",
    "configurations": [{
        "name": "Launch file",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${file}"
    },
    {
        "name": "Launch CTL",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/cmd/ctl/main.go",
        "args": [
            "--env:basedir=../../bin",
            "--env:workdir=../../work",
        ]
    },
    {
        "name": "Launch Repo",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/repo/cmd/main.go"
    },
    {
        "name": "Launch queue",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/srv/cmd/queue/main.go"
    },
    {
        "name": "Launch executor",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/srv/cmd/executor/main.go"
    },
    {
        "name": "Launch listener",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/srv/cmd/listener/main.go"
    },
    {
        "name": "Launch CLI Repo",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/tools/cmd/cli/main.go",
        "args": [
            "repo",
            "merch1",
            "personschema",
            "./schema.json"
        ]
    },
    {
        "name": "Launch CLI Deploy",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/tools/cmd/cli/main.go",
        "args": [
            "deploy",
            "-update",
            "C:/Users/Andres/projects/supportj/job1.yml",
        ]
    },
    {
        "name": "Launch CLI Env",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/tools/cmd/cli/main.go",
        "args": [
            "env",
            "./e.yml",
        ]
    },
    {
        "name": "Launch CLI Show",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/tools/cmd/cli/main.go",
        "args": [
            "show",
            "env",
        ]
    },
    {
        "name": "Launch CLI Recorder",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/tools/cmd/cli/main.go",
        "args": [
            "recorder",
            "-lines=3"
        ]
    },
    {
        "name": "Launch CLI",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/tools/cmd/cli/main.go",
        "args": [
            "show",
            "deploy",
            "test",
            "test",
        ]
    },
    {
        "name": "Launch Dashboard",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceFolder}/tools/cmd/dashboard/main.go",
        "console": "integratedTerminal",
        "args": [
            "-sync",
            "-debug",
        ]
    },
]
}